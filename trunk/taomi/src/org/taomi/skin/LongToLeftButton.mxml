<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark" xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:d="http://ns.adobe.com/fxg/2008/dt" width="13">
	<s:transitions>
		<s:Transition fromState="up" toState="over" autoReverse="true">
			<s:Parallel>
				<s:Parallel target="{bitmapimage1}">
					<s:Fade duration="100"/>
				</s:Parallel>
			</s:Parallel>
		</s:Transition>
		<s:Transition fromState="over" toState="up" autoReverse="true">
			<s:Parallel>
				<s:Parallel target="{bitmapimage1}">
					<s:Fade duration="100"/>
				</s:Parallel>
			</s:Parallel>
		</s:Transition>
		<s:Transition fromState="over" toState="down" autoReverse="true">
			<s:Parallel>
				<s:Parallel target="{bitmapimage1}">
					<s:Fade duration="100"/>
				</s:Parallel>
			</s:Parallel>
		</s:Transition>
	</s:transitions>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	<fx:Metadata>[HostComponent("spark.components.Button")]</fx:Metadata>
	<fx:DesignLayer d:userLabel="left button">
		<s:BitmapImage source="@Embed('/org/taomi/assets/image/bg.png')" d:userLabel="bg" width="13"/>
		<s:BitmapImage source="@Embed('/org/taomi/assets/image/hover.png')" d:userLabel="hover" visible.up="false" visible.down="false" id="bitmapimage1"  width="13"/>
		<s:BitmapImage source="@Embed('/org/taomi/assets/image/Layer_11_copy.png')" d:userLabel="Layer 11 copy" x="2" y="113" x.down="4"/>
	</fx:DesignLayer>
</s:Skin>